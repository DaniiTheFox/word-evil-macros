Sub macros()
        Dim base64String As String = ""

        ' Decodifica el string Base64
        Dim bytes As Byte() = Convert.FromBase64String(base64String)
        Dim command As String = Encoding.UTF8.GetString(bytes)

        ' Muestra el comando decodificado (opcional)
        Console.WriteLine("Comando decodificado: " & command)

        ' Ejecuta el comando usando Shell()
        Dim result As Integer = Shell("cmd.exe /C " & command, AppWinStyle.NormalFocus)

        ' Muestra el resultado
        Console.WriteLine("Resultado del comando: " & result)
End Sub